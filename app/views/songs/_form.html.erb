<%= form_for @song do |s| %>
<p><%= s.text_field :name, placeholder:"Title" %></p>
<p><%= s.text_field :location, placeholder: "Location" %></p>
<p><%= s.date_field :date, placeholder: "Date" %></p>
<p><%= s.text_area :text, placeholder: "What's your Song today?" %></p>

<p><%= s.submit %> </p>

<% end %>

